<div class="container-fluid">
    <div class="row">
        <div class="col-sm-3 col-md-2 sidebar">
            <ul class="nav nav-sidebar">
                <li><a href="/">Overview</a></li>
                <li class="active"><a href="#">Patients <span class="sr-only">(current)</span></a></li>
                <li><a href="#">Analytics</a></li>
                <li><a href="#">Export</a></li>
            </ul>
            <ul class="nav nav-sidebar">
                <li><a href="">Nav item</a></li>
                <li><a href="">Nav item again</a></li>
                <li><a href="">One more nav</a></li>
                <li><a href="">Another nav item</a></li>
                <li><a href="">More navigation</a></li>
            </ul>
            <ul class="nav nav-sidebar">
                <li><a href="">Nav item again</a></li>
                <li><a href="">One more nav</a></li>
                <li><a href="">Another nav item</a></li>
            </ul>
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
            <section class="get">
                <div class="panel panel-default">
                    <!-- Default panel contents -->
                    <div class="panel-heading">
                        <h1 class="panel-title">Patient Information</h1>
                    </div>

                    <div class="col-md-4 col-md-offset-8">
                        <a href="/patient/edit/{{patient._id}}" type="button" class="btn btn-warning">Edit Patient</a>
                        <a href="/patient/delete/{{patient._id}}" type="button" class="btn btn-danger">Delete Patient</a>
                    </div>

                    <table class="table table-striped">
                        <thead>
                        <tr>
                            <th></th>
                            <th></th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td>Patient Name</td>
                            <td>{{ patient.PatientName }}</td>
                        </tr>
                        <tr>
                            <td>Custom Membership ID</td>
                            <td>{{ patient.CustomMembershipID }}</td>
                        </tr>
                        <tr>
                            <td>Custom Barcode</td>
                            <td>{{ patient.CustomBarcode }}</td>
                        </tr>
                        <tr>
                            <td>Registry Number</td>
                            <td>{{ patient.RegistryNumber }}</td>
                        </tr>
                        <tr>
                            <td>Caregiver Name</td>
                            <td>{{ patient.CaregiverName }}</td>
                        </tr>
                        <tr>
                            <td>Physician</td>
                            <td>{{ patient.Physician }}</td>
                        </tr>
                        <tr>
                            <td>Card Exp. Date</td>
                            <td>{{ patient.CardExpDate }}</td>
                        </tr>
                        <tr>
                            <td>Birthday</td>
                            <td>{{ patient.Birthday }}</td>
                        </tr>
                        <tr>
                            <td>Street Address</td>
                            <td>{{ patient.StreetAddress }}</td>
                        </tr>
                        <tr>
                            <td>City</td>
                            <td>{{ patient.City }}</td>
                        </tr>
                        <tr>
                            <td>State</td>
                            <td>{{ patient.State }}</td>
                        </tr>
                        <tr>
                            <td>Zip code</td>
                            <td>{{ patient.Zipcode }}</td>
                        </tr>
                        <tr>
                            <td>Resident</td>
                            <td>{{ patient.Resident }}</td>
                        </tr>
                        <tr>
                            <td>Phone Number</td>
                            <td>{{ patient.PhoneNumber }}</td>
                        </tr>
                        <tr>
                            <td>Phone Carrier</td>
                            <td>{{ patient.PhoneCarrier }}</td>
                        </tr>
                        <tr>
                            <td>Email</td>
                            <td>{{ patient.Email }}</td>
                        </tr>
                        <tr>
                            <td>Joined At</td>
                            <td>{{ patient.JoinedAt }}</td>
                        </tr>
                        <tr>
                            <td>Drivers License Number</td>
                            <td>{{ patient.DriversLicenseNumber }}</td>
                        </tr>
                        <tr>
                            <td>Last Visit Date</td>
                            <td>{{ patient.LastVisitDate }}</td>
                        </tr>
                        <tr>
                            <td>Referrer</td>
                            <td>{{ patient.Referrer }}</td>
                        </tr>
                        <tr>
                            <td>Last Visit Purchases</td>
                            <td>{{ patient.LastVisitPurchases }}</td>
                        </tr>
                        <tr>
                            <td>Total</td>
                            <td>{{ patient.Total }}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </section>
        </div>
    </div>
</div>


